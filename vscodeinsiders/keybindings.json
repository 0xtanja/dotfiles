[
    // (Integrated) terminal stuff
    {
        "key": "ctrl+shift+`",
        "command": "workbench.action.terminal.newSelectWorkspace",
        "when": ""
    },
    {
        "key": "cmd+w",
        "command": "workbench.action.terminal.kill",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+t cmd+m",
        "command": "workbench.action.toggleMaximizedPanel",
        "when": "terminalFocus"
    }
]
